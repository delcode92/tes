<!-- main -->
<main_win_widget>
    <main_win_layout>
        
        <!-- top section -->
        <self.top_lay_widget>
            <self.top_lay>
                <self.burger_menu>
                </self.burger_menu>
            </self.top_lay>
        </self.top_lay_widget>
    
        <!-- content section -->
        <horizontal_widget>
            <horizontal_lay>

                <self.left_widget>
                    <left_menu_lay>
                        <!-- layout -->
                        
                        <left_menu_horizontal>
                            <self.home_btn/>
                            <self.home_lbl/>
                        </left_menu_horizontal>    
                        
                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.rfid_btn/>
                            <self.rfid_lbl/>
                        </left_menu_horizontal>
                        
                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.users_btn/>
                            <self.users_lbl/>
                        </left_menu_horizontal>

                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.kasir_btn/>
                            <self.kasir_lbl/>
                        </left_menu_horizontal>

                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.karcis_btn/>
                            <self.karcis_lbl/>
                        </left_menu_horizontal>

                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.tarif_btn/>
                            <self.tarif_lbl/>
                        </left_menu_horizontal>

                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.voucher_btn/>
                            <self.voucher_lbl/>
                        </left_menu_horizontal>

                        <!-- re-initialize -->
                        <left_menu_horizontal>
                            <self.laporan_btn/>
                            <self.laporan_lbl/>
                        </left_menu_horizontal>

                    </left_menu_lay>    
                </self.left_widget>
                
                <right_widget>
                    <self.right_content_lay>
                        <self.stacked_widget>
                            <self.welcome_lbl/>
                            
                            <!-- RFID -->
                            <self.rfid_container>
                                <self.rfid_container_lay>

                                    <rfid_tabs_container_widget>
                                        
                                        <!-- add widgets via method setTabButton() -->
                                        <rfid_tabs_container>
                                            <tab1/>
                                            <tab2/>
                                        </rfid_tabs_container>

                                    </rfid_tabs_container_widget>    
                                    
                                    <self.rfid_stack>
                                        <rfid_content1>
                                            <rfid_content1_lay>
                                                <tab1_h_widget>
                                                    <tab1_h_layout>
                                                        <table/>
                                                        <action_widget>
                                                            <action_lay>
                                                                <row_label/>
                                                                <row_info/>
                                                                <row_edit/>
                                                                <row_delete/>
                                                            </action_lay>
                                                        </action_widget>
                                                    </tab1_h_layout>
                                                </tab1_h_widget>
                                            </rfid_content1_lay>
                                        </rfid_content1>
                                        
                                        <!-- rfid_content2 as widget container -->
                                        <rfid_content2>
                                            <rfid_content2_lay>
                                                
                                                <form_container>
                                                    <form_container_lay>

                                                        <!-- component setter here -->
                                                        ....

                                                        <!-- create components here-->
                                                        <self.CreateComponentLayout(components_setter, form_container_lay)/>

                                                    </form_container_lay>
                                                </form_container>
                                                
                                                <!-- rfid_content2 as QLabel -->
                                                <rfid_content2/>
                                                    
                                            </rfid_content2_lay>    
                                        </rfid_content2>

                                    </self.rfid_stack>

                                </self.rfid_container_lay>    
                            </self.rfid_container>


                            <!-- USERS -->
                            <self.users_container>
                                <self.users_container_lay>

                                    <users_tabs_container_widget>
                                        
                                        <!-- add widgets via method setTabButton() -->
                                        <users_tabs_container>
                                            <tab1/>
                                            <tab2/>
                                        </users_tabs_container>

                                    </users_tabs_container_widget>    
                                    
                                    <self.users_stack>
                                        <users_content1>
                                            <users_content1_lay>
                                                <tab1_h_widget>
                                                    <tab1_h_layout>
                                                        <table/>
                                                        <action_widget>
                                                            <action_lay>
                                                                <row_label/>
                                                                <row_info/>
                                                                <row_edit/>
                                                                <row_delete/>
                                                            </action_lay>
                                                        </action_widget>
                                                    </tab1_h_layout>
                                                </tab1_h_widget>
                                            </users_content1_lay>
                                        </users_content1>
                                        
                                        <!-- users_content2 as widget container -->
                                        <users_content2>
                                            <users_content2_lay>
                                                
                                                <form_container>
                                                    <form_container_lay>

                                                        <!-- component setter here -->
                                                        ....

                                                        <!-- create components here-->
                                                        <self.CreateComponentLayout(components_setter, form_container_lay)/>

                                                    </form_container_lay>
                                                </form_container>
                                                
                                                <!-- users_content2 as QLabel -->
                                                <users_content2/>
                                                    
                                            </users_content2_lay>    
                                        </users_content2>

                                    </self.users_stack>

                                </self.users_container_lay>  
                            </self.users_container>


                            <!-- KASIR -->
                            <self.kasir_container>
                                ...
                            </self.kasir_container>
                            
                            <!-- KARCIS -->
                            <self.karcis_container>
                                ...
                            </self.karcis_container>
                            
                            <!-- TARIF -->
                            <self.tarif_container>
                                ...
                            </self.tarif_container>
                            
                            <!-- VOUCHER -->
                            <self.voucher_container>
                                ...
                            </self.voucher_container>
                            
                            <!-- LAPORAN -->
                            <self.laporan_container>
                                ...
                            </self.laporan_container>

                        </self.stacked_widget>
                    </self.right_content_lay>
                </right_widget>

            </horizontal_lay>
        </horizontal_widget>

    </main_win_layout>
</main_win_widget>

